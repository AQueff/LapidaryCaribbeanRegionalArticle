@article{georjon_late_2021,
  title = {Late {{Neolithic}} to {{Early-Mid Bronze Age}} Semi-Precious Stone Bead Production and Consumption at {{Oakaie}} and {{Nyaung}}'gan in Central-Northern {{Myanmar}}},
  author = {Georjon, Clo{\'e} and Kyaw, U. Aung Aung and Win, Daw Tin Tin and Win, Daw Thu Thu and Pradier, Baptiste and Willis, Anna and Petchey, Peter and Iizuka, Yoshiyuki and Gonthier, Eric and Pelegrin, Jacques and Bellina, B{\'e}r{\'e}nice and Pryce, T. O.},
