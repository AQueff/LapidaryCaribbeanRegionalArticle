@article{nagele_genomic_2020,
  title = {Genomic Insights into the Early Peopling of the {{Caribbean}}},
  author = {N{\"a}gele, Kathrin and Posth, Cosimo and Orbegozo, Miren Iraeta and de Armas, Yadira Chinique and Godoy, Silvia Teresita Hern{\'a}ndez and Herrera, Ulises M. Gonz{\'a}lez and {Nieves-Col{\'o}n}, Maria A. and {Sandoval-Velasco}, Marcela and Mylopotamitaki, Dorothea and Radzeviciute, Rita and Laffoon, Jason and Pestle, William J. and {Ramos-Madrigal}, Jazmin and Lamnidis, Thiseas C. and Schaffer, William C. and Carr, Robert S. and Day, Jane S. and Ant{\'u}nez, Carlos Arredondo and Rivero, Armando Rangel and {Mart{\'i}nez-Fuentes}, Antonio J. and {Crespo-Torres}, Edwin and Roksandic, Ivan and Stone, Anne C. and {Lalueza-Fox}, Carles and Hoogland, Menno and Roksandic, Mirjana and Hofman, Corinne L. and Krause, Johannes and Schroeder, Hannes},
