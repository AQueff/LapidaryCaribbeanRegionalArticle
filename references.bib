@article{derrico_zhoukoudian_2021,
  title = {Zhoukoudian {{Upper Cave}} Personal Ornaments and Ochre: {{Rediscovery}} and Reevaluation},
  shorttitle = {Zhoukoudian {{Upper Cave}} Personal Ornaments and Ochre},
  author = {{d'Errico}, Francesco and Pitarch Mart{\'i}, Africa and Wei, Yi and Gao, Xing and Vanhaeren, Marian and Doyon, Luc},
  year = {2021},
  journal = {Journal of Human Evolution},
  volume = {161},
  pages = {103088},
  issn = {0047-2484},
  doi = {10.1016/j.jhevol.2021.103088},
  urldate = {2022-02-18},
  abstract = {Personal ornaments have become a key cultural proxy to investigate cognitive evolution, modern human dispersal, and population dynamics. Here, we reassess personal ornaments found at Zhoukoudian Upper Cave and compare them with those from other Late Paleolithic Northern Chinese sites. We reappraise the information provided by Pei Wen Chung on Upper Cave personal ornaments lost during World War II and analyze casts of 17 of them, along with two unpublished objects displayed at the Zhoukoudian Site Museum and three original perforated teeth rediscovered at the Zhoukoudian Site Museum. We apply archeozoological, technological and use-wear analyses to document variation in ornamental practices and their change throughout the site stratigraphy. Badger, fox, red deer, sika deer, marten, and tiger teeth as well as carp bone, bird bone, Anadara shell, limestone beads, and perforated pebble appear to have been the preferred objects used as ornaments by Upper Cave visitors. Multivariate analysis of technological data highlights a correspondence between cultural layers and perforation techniques, with radial incising being typical of layer L2 and bidirectional incising of L4. The three rediscovered badger canines display features suggesting they were sewed on clothing rather than suspended from necklaces or bracelets. Elemental scanning electron microscopy coupled with energy dispersive X-ray spectromety and mineralogical ({$\mu$}-Raman) analyses of red residues adhering to the rediscovered teeth indicate these objects were originally coated with ochre and identify variations that match differences in technology. The two ornaments exhibited at the Zhoukoudian Site Museum are ancient teeth that were recently perforated and should be excluded from the Upper Cave assemblage. A seriation of Late Paleolithic ornaments found at Northern Chinese sites identifies a clear-cut difference in preferred ornament types between western and eastern sites, interpreted as reflecting two long-lasting traditions in garment symbolic codes.},
  langid = {english},
  keywords = {East Asia,Fake identification,Residue analysis,Symbolism,Upper Paleolithic},
  file = {C\:\\Users\\a.queffelec\\Zotero\\storage\\R39QGNPQ\\d’Errico et al_2021_Zhoukoudian Upper Cave personal ornaments and ochre.pdf;C\:\\Users\\a.queffelec\\Zotero\\storage\\6MGNX8JQ\\S0047248421001408.html}
}

@article{earle_political_2015,
